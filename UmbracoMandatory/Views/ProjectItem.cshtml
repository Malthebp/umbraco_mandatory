@inherits Umbraco.Web.Mvc.UmbracoTemplatePage
@{
	Layout = "Master.cshtml";
}


<article class="project-item">
    <div class="container">
        <h1>
            @Model.Content.GetPropertyValue("title")
        </h1>
    </div>
    <div class="project-item__content">
        @Html.GetGridHtml(Model.Content, "layout")
    </div>
</article>
